{
  "name": "realeagent-prototype",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web-app",
    "api-server",
    "shared"
  ],
  "scripts": {
    "dev": "cd web-app && npm run dev",
    "dev:api": "cd api-server && npm run dev",
    "build": "cd web-app && npm run build",
    "build:api": "cd api-server && npm run build",
    "test": "npm run test:web && npm run test:api",
    "test:web": "cd web-app && npm test",
    "test:api": "cd api-server && npm test",
    "install:all": "npm install && cd web-app && npm install && cd ../api-server && npm install",
    "clean": "rm -rf node_modules web-app/node_modules api-server/node_modules",
    "clean:install": "npm run clean && npm run install:all",
    "demo:reset": "curl -X POST http://localhost:3001/api/v1/auth/demo/reset",
    "demo:test": "curl http://localhost:3001/api/v1/properties/search?q=ML81234567",
    "update:dates": "./update-dates.sh"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
