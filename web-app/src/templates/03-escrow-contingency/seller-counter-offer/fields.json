      "required": false,
      "section": "additional",
      "maxLength": 300,
      "description": "List of personal property items included in sale"
    },
    {
      "name": "personalPropertyExcluded",
      "type": "text",
      "label": "Personal Property Excluded",
      "required": false,
      "section": "additional",
      "maxLength": 300,
      "description": "List of personal property items excluded from sale"
    },
    {
      "name": "additionalTerms",
      "type": "text",
      "label": "Additional Terms and Conditions",
      "required": false,
      "section": "additional",
      "maxLength": 500,
      "rows": 4
    },
    {
      "name": "counterOfferExpiration",
      "type": "datetime",
      "label": "This Counter Offer Expires At",
      "required": true,
      "section": "expiration",
      "description": "Date and time when this counter offer expires"
    },
    {
      "name": "sellerPossessionAfterClose",
      "type": "number",
