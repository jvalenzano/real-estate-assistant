    },
    {
      "name": "initialDeposit",
      "type": "currency",
      "label": "Initial Deposit",
      "required": true,
      "section": "terms",
      "min": 0
    },
    {
      "name": "escrowPeriod",
      "type": "number",
      "label": "Escrow Period (Days)",
      "required": true,
      "section": "terms",
      "min": 1,
      "max": 180,
      "default": 30
