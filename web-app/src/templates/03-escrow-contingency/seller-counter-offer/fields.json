    },
    {
      "name": "propertyAddress",
      "type": "string",
      "label": "Property Address",
      "required": true,
      "section": "property",
      "maxLength": 100
    },
    {
      "name": "propertyCity",
      "type": "string",
      "label": "City",
      "required": true,
      "section": "property",
      "maxLength": 50
    },
    {
      "name": "propertyCounty",
      "type": "string",
      "label": "County",
      "required": true,
      "section": "property",
      "maxLength": 30
    },
    {
      "name": "propertyZip",
      "type": "string",
      "label": "ZIP Code",
      "required": true,
      "section": "property",
      "pattern": "^[0-9]{5}(-[0-9]{4})?$"
    },
    {
      "name": "sellerName",
      "type": "string",
      "label": "Seller Name(s)",
      "required": true,
